{
	"name": "Jira Harvest Time Tracking",
	"version": "1.1",
	"description": "Adds a Harvest Button to JIRA",
	"content_scripts": [
		{
			"matches": ["https://*/projects/*/issues/*",
                  "https://*/browse/*"],
			"js": ["jquery-2.1.4.min.js","jiraharvest.js"]
		}
	],
	"icons": { "128": "harvest-jira-icon.png" },
  "manifest_version" : 2,
  "web_accessible_resources": ["images/timer-icon.png"]

}
